    <owl:Class rdf:about="http://schema.org/Recipe">
        <rdfs:subClassOf rdf:resource="http://schema.org/HowTo"/>
        <rdfs:comment xml:lang="en">A recipe. For dietary restrictions covered by the recipe, a few common restrictions are enumerated via &lt;a class=&quot;localLink&quot; href=&quot;http://schema.org/suitableForDiet&quot;&gt;suitableForDiet&lt;/a&gt;. The &lt;a class=&quot;localLink&quot; href=&quot;http://schema.org/keywords&quot;&gt;keywords&lt;/a&gt; property can also be used to add more detail.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://schema.org/Recipe"/>
        <rdfs:label xml:lang="en">Recipe</rdfs:label>
    </owl:Class>
    


    <!-- http://schema.org/RestrictedDiet -->


    <owl:Class rdf:about="http://schema.org/RestrictedDiet">
        <rdfs:subClassOf rdf:resource="http://schema.org/Enumeration"/>
        <rdfs:comment xml:lang="en">A diet restricted to certain foods or preparations for cultural, religious, health or lifestyle reasons.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://schema.org/RestrictedDiet"/>
        <rdfs:label xml:lang="en">RestrictedDiet</rdfs:label>
    </owl:Class>
    


