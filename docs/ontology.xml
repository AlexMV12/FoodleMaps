    <!-- https://alexmv12.github.io/FoodleMaps/#hasPreparationTime -->


    <owl:DatatypeProperty rdf:about="https://alexmv12.github.io/FoodleMaps/#hasPreparationTime">
        <rdfs:domain rdf:resource="http://schema.org/MenuItem"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <vann:example rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">10.</vann:example>
        <rdfs:comment>Indicates how many minutes are needed to prepare and serve the dish. More precisely, is the time which should pass between the moment in which the order is made and the moment in which the dish is server.</rdfs:comment>
        <rdfs:label>hasPreparationTime</rdfs:label>
    </owl:DatatypeProperty>
    


