        <rdfs:label>Characteristic</rdfs:label>
    </owl:Class>
    


    <!-- https://alexmv12.github.io/FoodleMaps/#Ingredient -->


    <owl:Class rdf:about="https://alexmv12.github.io/FoodleMaps/#Ingredient">
