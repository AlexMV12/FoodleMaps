    <!-- http://schema.org/hasMenu -->


    <owl:ObjectProperty rdf:about="http://schema.org/hasMenu">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://schema.org/FoodEstablishment"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://schema.org/Menu"/>
                    <rdf:Description rdf:about="http://schema.org/Role"/>
                    <rdf:Description rdf:about="http://schema.org/Text"/>
                    <rdf:Description rdf:about="http://schema.org/URL"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Either the actual menu as a structured representation, as text, or a URL of the menu.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://schema.org/hasMenu"/>
        <rdfs:label xml:lang="en">hasMenu</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/hasMenuItem -->


    <owl:ObjectProperty rdf:about="http://schema.org/hasMenuItem">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://schema.org/Menu"/>
                    <rdf:Description rdf:about="http://schema.org/MenuSection"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://schema.org/MenuItem"/>
                    <rdf:Description rdf:about="http://schema.org/Role"/>
                    <rdf:Description rdf:about="http://schema.org/Text"/>
                    <rdf:Description rdf:about="http://schema.org/URL"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">A food or drink item contained in a menu or menu section.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://schema.org/hasMenuItem"/>
        <rdfs:label xml:lang="en">hasMenuItem</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/suitableForDiet -->


    <owl:ObjectProperty rdf:about="http://schema.org/suitableForDiet">
        <rdfs:domain rdf:resource="http://schema.org/Recipe"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://schema.org/RestrictedDiet"/>
                    <rdf:Description rdf:about="http://schema.org/Role"/>
                    <rdf:Description rdf:about="http://schema.org/Text"/>
                    <rdf:Description rdf:about="http://schema.org/URL"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Indicates a dietary restriction or guideline for which this recipe or menu item is suitable, e.g. diabetic, halal etc.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://schema.org/suitableForDiet"/>
        <rdfs:label xml:lang="en">suitableForDiet</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://alexmv12.github.io/FoodleMaps/#hasCharacteristic -->


    <owl:ObjectProperty rdf:about="https://alexmv12.github.io/FoodleMaps/#hasCharacteristic">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://schema.org/MenuItem"/>
        <rdfs:range rdf:resource="https://alexmv12.github.io/FoodleMaps/#Characteristic"/>
        <vann:example>Assigns &quot;Light&quot; to &quot;pasta al pomodoro&quot;.</vann:example>
        <rdfs:comment>Assigns one characteristic to a dish.</rdfs:comment>
        <rdfs:label>hasCharacteristic</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://alexmv12.github.io/FoodleMaps/#hasIngredient -->


    <owl:ObjectProperty rdf:about="https://alexmv12.github.io/FoodleMaps/#hasIngredient">
        <rdfs:domain rdf:resource="http://schema.org/Recipe"/>
        <rdfs:range rdf:resource="https://alexmv12.github.io/FoodleMaps/#Ingredient"/>
        <vann:example>Associates &quot;Spaghetti&quot; ingredient to &quot;Pasta al pomodoro&quot;.</vann:example>
        <rdfs:comment>Associates an ingredient to a recipe.</rdfs:comment>
        <rdfs:label>hasIngredient</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://alexmv12.github.io/FoodleMaps/#hasRecipe -->


    <owl:ObjectProperty rdf:about="https://alexmv12.github.io/FoodleMaps/#hasRecipe">
        <rdfs:domain rdf:resource="http://schema.org/MenuItem"/>
        <rdfs:range rdf:resource="http://schema.org/Recipe"/>
        <rdfs:comment>Associates a Recipe to a MenuItem.MenuItems can have multiple recipes, to indicate different versions of the same dish.</rdfs:comment>
        <rdfs:label>hasRecipe</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://alexmv12.github.io/FoodleMaps/#requiresCharacteristic -->


    <owl:ObjectProperty rdf:about="https://alexmv12.github.io/FoodleMaps/#requiresCharacteristic">
        <rdfs:domain rdf:resource="https://alexmv12.github.io/FoodleMaps/#User"/>
        <rdfs:range rdf:resource="https://alexmv12.github.io/FoodleMaps/#Characteristic"/>
        <vann:example>The user wants a &quot;Heavy&quot; dish.</vann:example>
        <rdfs:comment>Used to express a constraint wanted by the user to a specific characteristic.</rdfs:comment>
        <rdfs:label>requiresCharacteristic</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://alexmv12.github.io/FoodleMaps/#requiresDiet -->


    <owl:ObjectProperty rdf:about="https://alexmv12.github.io/FoodleMaps/#requiresDiet">
        <rdfs:domain rdf:resource="https://alexmv12.github.io/FoodleMaps/#User"/>
        <rdfs:range rdf:resource="http://schema.org/RestrictedDiet"/>
        <vann:example>Restricts dishes to VeganDiet.</vann:example>
        <rdfs:comment>Used to express a diet followed by the user; the returned dishes will have to be suitable for that diet.</rdfs:comment>
        <rdfs:label>requiresDiet</rdfs:label>
    </owl:ObjectProperty>
    


