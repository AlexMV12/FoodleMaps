#################################################################
#    Object Properties
#################################################################

###  http://schema.org/hasMenu
<http://schema.org/hasMenu> rdf:type owl:ObjectProperty ;
                            rdfs:domain [ rdf:type owl:Class ;
                                          owl:unionOf ( <http://schema.org/FoodEstablishment>
                                                      )
                                        ] ;
                            rdfs:range [ rdf:type owl:Class ;
                                         owl:unionOf ( <http://schema.org/Menu>
                                                       <http://schema.org/Role>
                                                       <http://schema.org/Text>
                                                       <http://schema.org/URL>
                                                     )
                                       ] ;
                            rdfs:comment "Either the actual menu as a structured representation, as text, or a URL of the menu."@en ;
                            rdfs:isDefinedBy <https://schema.org/hasMenu> ;
                            rdfs:label "hasMenu"@en .


###  http://schema.org/hasMenuItem
<http://schema.org/hasMenuItem> rdf:type owl:ObjectProperty ;
                                rdfs:domain [ rdf:type owl:Class ;
                                              owl:unionOf ( <http://schema.org/Menu>
                                                            <http://schema.org/MenuSection>
                                                          )
                                            ] ;
                                rdfs:range [ rdf:type owl:Class ;
                                             owl:unionOf ( <http://schema.org/MenuItem>
                                                           <http://schema.org/Role>
                                                           <http://schema.org/Text>
                                                           <http://schema.org/URL>
                                                         )
                                           ] ;
                                rdfs:comment "A food or drink item contained in a menu or menu section."@en ;
                                rdfs:isDefinedBy <https://schema.org/hasMenuItem> ;
                                rdfs:label "hasMenuItem"@en .


###  http://schema.org/suitableForDiet
<http://schema.org/suitableForDiet> rdf:type owl:ObjectProperty ;
                                    rdfs:domain <http://schema.org/Recipe> ;
                                    rdfs:range [ rdf:type owl:Class ;
                                                 owl:unionOf ( <http://schema.org/RestrictedDiet>
                                                               <http://schema.org/Role>
                                                               <http://schema.org/Text>
                                                               <http://schema.org/URL>
                                                             )
                                               ] ;
                                    rdfs:comment "Indicates a dietary restriction or guideline for which this recipe or menu item is suitable, e.g. diabetic, halal etc."@en ;
                                    rdfs:isDefinedBy <https://schema.org/suitableForDiet> ;
                                    rdfs:label "suitableForDiet"@en .


###  http://www.semanticweb.org/FoodleMaps#hasCharacteristic
:hasCharacteristic rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain <http://schema.org/MenuItem> ;
                   rdfs:range :Characteristic ;
                   <http://purl.org/vocab/vann/example> "Assigns \"Light\" to \"pasta al pomodoro\"." ;
                   rdfs:comment "Assigns one characteristic to a dish." ;
                   rdfs:label "hasCharacteristic" .


###  http://www.semanticweb.org/FoodleMaps#hasIngredient
:hasIngredient rdf:type owl:ObjectProperty ;
               rdfs:domain <http://schema.org/Recipe> ;
               rdfs:range :Ingredient ;
               <http://purl.org/vocab/vann/example> "Associates \"Spaghetti\" ingredient to \"Pasta al pomodoro\"." ;
               rdfs:comment "Associates an ingredient to a recipe." ;
               rdfs:label "hasIngredient" .


###  http://www.semanticweb.org/FoodleMaps#hasRecipe
:hasRecipe rdf:type owl:ObjectProperty ;
           rdfs:domain <http://schema.org/MenuItem> ;
           rdfs:range <http://schema.org/Recipe> ;
           rdfs:comment "Associates a Recipe to a MenuItem.MenuItems can have multiple recipes, to indicate different versions of the same dish." ;
           rdfs:label "hasRecipe" .


###  http://www.semanticweb.org/FoodleMaps#requiresCharacteristic
:requiresCharacteristic rdf:type owl:ObjectProperty ;
                        rdfs:domain :User ;
                        rdfs:range :Characteristic ;
                        <http://purl.org/vocab/vann/example> "The user wants a \"Heavy\" dish." ;
                        rdfs:comment "Used to express a constraint wanted by the user to a specific characteristic." ;
                        rdfs:label "requiresCharacteristic" .


###  http://www.semanticweb.org/FoodleMaps#requiresDiet
:requiresDiet rdf:type owl:ObjectProperty ;
              rdfs:domain :User ;
              rdfs:range <http://schema.org/RestrictedDiet> ;
              <http://purl.org/vocab/vann/example> "Restricts dishes to VeganDiet." ;
              rdfs:comment "Used to express a diet followed by the user; the returned dishes will have to be suitable for that diet." ;
              rdfs:label "requiresDiet" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/FoodleMaps#hasPreparationTime
:hasPreparationTime rdf:type owl:DatatypeProperty ;
                    rdfs:domain <http://schema.org/MenuItem> ;
                    rdfs:range xsd:int ;
                    <http://purl.org/vocab/vann/example> 10. ;
                    rdfs:comment "Indicates how many minutes are needed to prepare and serve the dish. More precisely, is the time which should pass between the moment in which the order is made and the moment in which the dish is server." ;
                    rdfs:label "hasPreparationTime" .


